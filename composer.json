{
    "name": "picqer/exact-php-client",
    "type": "library",
    "description": "A PHP Client for the Exact API",
    "keywords": [
        "api",
        "php",
        "exact"
    ],
    "homepage": "http://github.com/picqer/exact-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Groen",
            "email": "info@picqer.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "guzzlehttp/guzzle": "~6.0|~7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Picqer\\": "src/Picqer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Picqer\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "exact-php-client": {
            "type": "vcs",
            "url": "https://github.com/vaughan-dsouza/exact-php-client"
        }
    }
}
