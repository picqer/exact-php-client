{
    "name": "picqer/exact-php-client",
    "type": "library",
    "description": "A PHP Client for the Exact API",
    "keywords": [
        "api",
        "php",
        "exact"
    ],
    "homepage": "http://github.com/picqer/exact-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Groen",
            "email": "info@picqer.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36"
    },
    "autoload": {
        "psr-4": {
            "Picqer\\": "src/Picqer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Picqer\\Tests\\": "tests/"
        }
    }
}
